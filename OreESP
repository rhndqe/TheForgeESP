local ProcessedOres = {}
local PlayerESP = {}
local SelectedPlayer = nil
local PlayerESPEnabled = false

-- Teleport Configuration
local TeleportConfig = {
    ["Farm Spot"] = {
        enabled = false,
        position = Vector3.new(719.2117309570312, 48.360374450683594, 103.62264251708984),
        intermediateY = 10, -- Y position for first tween (going down)
        duration1 = 0.5, -- Duration for first tween (to intermediate position)
        duration2 = 1.0, -- Duration for second tween (to final position)
        easingStyle = Enum.EasingStyle.Quad,
        easingDirection = Enum.EasingDirection.Out
    }
}

